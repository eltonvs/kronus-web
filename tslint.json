{
  "defaultSeverity": "error",
  "extends": [
    "tslint-react"
  ],
  "jsRules": {},
  "rules": {
    "member-access": false,
    "ordered-imports": true,
    "quotemark": false,
    "no-console": false,
    "semicolon": false,
    "jsx-no-lambda": false,
    "jsx-boolean-value": false,
    "jsx-no-multiline-js": false
  },
  "rulesDirectory": [],
  "linterOptions": {
    "exclude": [
      "node_modules/**/*.ts"
    ]
  }
}
